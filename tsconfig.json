{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/shared": [
        "libs/shared/src"
      ],
      "@app/shared/*": [
        "libs/shared/src/*"
      ],
      "@app/config": [
        "libs/config/src"
      ],
      "@app/config/*": [
        "libs/config/src/*"
      ],
      "@app/guards": [
        "libs/guards/src"
      ],
      "@app/guards/*": [
        "libs/guards/src/*"
      ],
      "@app/pipes": [
        "libs/pipes/src"
      ],
      "@app/pipes/*": [
        "libs/pipes/src/*"
      ],
      "@app/interfaces": [
        "libs/interfaces/src"
      ],
      "@app/interfaces/*": [
        "libs/interfaces/src/*"
      ],
      "@app/auth-shared": [
        "libs/auth-shared/src"
      ],
      "@app/auth-shared/*": [
        "libs/auth-shared/src/*"
      ],
      "@app/microservices": [
        "libs/microservices/src"
      ],
      "@app/microservices/*": [
        "libs/microservices/src/*"
      ],
      "@app/profile-shared": [
        "libs/profile-shared/src"
      ],
      "@app/profile-shared/*": [
        "libs/profile-shared/src/*"
      ]
    }
  }
}